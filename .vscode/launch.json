{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "ts-node",
      "type": "node",
      "request": "launch",
      "args": [
        "${relativeFile}"
      ],
      "runtimeArgs": [
        "-r",
        "ts-node/register"
      ],
      "cwd": "${workspaceRoot}",
      "internalConsoleOptions": "openOnSessionStart"
    },
    {
      "type": "node",
      "name": "Run Current File",
      "request": "launch",
      "program": "${workspaceFolder}/src/eval.ts"
    }
  ]
}